            "problemMatcher": "$tsc",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Note Publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/CQ.Note/CQ.Note.Api/CQ.Note.Api.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "Note Watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/CQ.Note/CQ.Note.Api/CQ.Note.Api.csproj"
            ],
            "problemMatcher": "$tsc"
        }
    ]
}